Class {
	#name : #CuOOra,
	#superclass : #Object,
	#instVars : [
		'usuarios',
		'topicos'
	],
	#category : #'OO2-CuOOra'
}

{ #category : #adding }
CuOOra >> agregarTopicoConNombre: unNombre yDescripcion: unaDescripcion [
	topicos add: (Topico nombre: unNombre descripcion: unaDescripcion).
]

{ #category : #adding }
CuOOra >> crearUsuario: unNombre conContrase単a: unaContrase単a [
	usuarios add: (Usuario nombre: unNombre contrase単a: unaContrase単a fechaYhora: DateAndTime current).
]

{ #category : #initialize }
CuOOra >> initialize [ 
	usuarios := OrderedCollection new.
	topicos := OrderedCollection new.
]

{ #category : #accessing }
CuOOra >> topicos [
	^ topicos
]

{ #category : #accessing }
CuOOra >> usuarios [
	^ usuarios
]
